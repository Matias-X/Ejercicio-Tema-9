public class Main {
    public static void main (String[] args) {
       Cliente cliente = new Cliente();
       cliente.edad = 25;
       cliente.nombre = "Matias";
       cliente.telefono = 123123;
       cliente.credito = 200000;
       
       Trabajador trabajador = new Trabajador();
       trabajador.salario = 15000;
       
       System.out.println("Tu edad es: "+cliente.edad);
       System.out.println("Tu nombre es: "+cliente.nombre);
       System.out.println("Tu numero de telefono es: "+cliente.telefono);
       System.out.println("Tu credito es de: "+cliente.credito);
       System.out.println("Tu salario es de: "+trabajador.salario);
       }
}

class Persona {
     int edad;
     String nombre;
     int telefono;
   
}

class Cliente extends Persona {
    
     int credito;
    
    
}

class Trabajador extends Persona {
    
    int salario;
    
}


